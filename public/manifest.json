{
  "manifest_version": 2,
  "name": "Unsplash Tabs",
  "author": "Martin Burdon",
  "version": "0.0.1",
  "description": "Replace new tab screen with a random image from Unsplash.",
  "incognito": "split",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}
